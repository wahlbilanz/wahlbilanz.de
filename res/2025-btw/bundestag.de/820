data-img-xs-normal="/resource/image/862590/3x4/113/151/e3e3f4cdacbaa8618f2d06ca81c171f6/1BA209D4E532F5A1AB553B44CD1C4497/merz_friedrich.jpg"
data-img-xs-retina="/resource/image/862590/3x4/226/302/e3e3f4cdacbaa8618f2d06ca81c171f6/4491E88C7765BC71B61829F54208CCC7/merz_friedrich.jpg"
